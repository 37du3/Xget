{
  "functions": {
    "api/[...path].js": {
      "runtime": "@vercel/edge@4.0.0"  // 正确的版本格式，使用最新稳定版
    }
  },
  "headers": [
    {
      "headers": [
        {
          "key": "X-Powered-By",
          "value": "Xget on Vercel"
        }
      ],
      "source": "/(.*)"
    }
  ],
  "rewrites": [
    {
      "destination": "/api/$1",
      "source": "/(.*)"
    }
  ]
}

