{
  "functions": {
    "api/[...path].js": {
      "runtime": "edge@1.5.0"  // 补充 Edge 运行时版本号
    }
  },
  "headers": [
    {
      "headers": [
        {
          "key": "X-Powered-By",
          "value": "Xget on Vercel"
        }
      ],
      "source": "/(.*)"
    }
  ],
  "rewrites": [
    {
      "destination": "/api/$1",
      "source": "/(.*)"
    }
  ]
}
